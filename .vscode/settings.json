{
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.tsc.autoDetect": "off",
	"json.schemas": [
		{
			"fileMatch": ["/*electron-builder.json5", "/*electron-builder.json"],
			"url": "https://json.schemastore.org/electron-builder"
		}
	],
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true
	},
	"hide-files.files": []
}
